ans = input()
counter = 1
i = 0
while i < len(ans) - 1:
    if ans[i] == ans[i + 1]:
        counter += 1
    else:
        print(ans[i], sep="", end="")
        print(counter, sep="", end="")
        counter = 1
    i += 1
j = 2
while ans[len(ans) - 1] == ans[len(ans) - j]:
    j += 1
print(ans[len(ans) - 1], sep="", end="")
print(j - 1, sep="", end="")
